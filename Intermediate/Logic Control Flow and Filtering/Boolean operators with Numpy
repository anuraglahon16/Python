# Create arrays
import numpy as np
my_gate= np.array([18.0, 20.0, 10.75, 9.50])
your_gate = np.array([14.0, 24.0, 14.25, 9.0])

# my_gate greater than 18.5 or smaller than 10
print(np.logical_or(my_gate>18.5,my_gate<10))

# Both my_gate and your_gate smaller than 11
print(np.logical_and(my_gate<11,your_gate<11))
